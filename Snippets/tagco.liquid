<!-- TagCommander script asynchronous loading -->
{% include 'dl_ca' %}
{% if settings.ca_account_id and settings.ca_tagco_enabled and settings.ca_container_filename %}
<script type="text/javascript">
    (function(a,b,c,d){
      	a='//cdn.tagcommander.com/{{ settings.ca_account_id }}{{ settings.ca_environment }}/{{ settings.ca_container_filename }}';
    	b=document;c='script';d=b.createElement(c);d.src=a;d.type='text/java'+c;d.async=true;
    	b.body.appendChild(d);
    })();
</script>
{% endif %}